{
	"name": "svelte-animate-icons",
	"version": "1.2.1",
	"type": "module",
	"description": "Beautiful, performant animated SVG icons for Svelte 5 with runes",
	"author": {
		"name": "Serhat YILDIZ",
		"email": "serhatgulcanyldz04@gmail.com",
		"url": "https://github.com/serhat-yildiz"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/serhat-yildiz/svelte-animate-icons.git"
	},
	"homepage": "https://serhat-yildiz.github.io/svelte-animate-icons/",
	"bugs": {
		"url": "https://github.com/serhat-yildiz/svelte-animate-icons/issues"
	},
	"keywords": [
		"svelte",
		"svelte5",
		"icons",
		"animations",
		"svg",
		"runes",
		"web-animations-api",
		"typescript",
		"ui-components"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "publint"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"svelte": "./dist/index.js",
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"dependencies": {
		"@fontsource/inter": "^5.2.6",
		"clsx": "^2.1.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.36.2",
		"@sveltejs/package": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"publint": "^0.3.12",
		"svelte": "^5.0.0",
		"svelte-check": "^3.8.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.0"
	}
}
