{
	"name": "svelte-animate-icons",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"description": "Beautiful, performant animated SVG icons for Svelte 5 with runes",
	"author": {
		"name": "Serhat YILDIZ",
		"email": "serhatgulcanyldz04@gmail.com",
		"url": "https://github.com/serhat-yildiz"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/serhat-yildiz/svelte-animate-icons.git"
	},
	"homepage": "https://github.com/serhat-yildiz/svelte-animate-icons#readme",
	"bugs": {
		"url": "https://github.com/serhat-yildiz/svelte-animate-icons/issues"
	},
	"keywords": [
		"svelte",
		"svelte5",
		"icons",
		"animations",
		"svg",
		"runes",
		"web-animations-api",
		"typescript",
		"ui-components"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"convert-icons": "node scripts/specific-converter.js",
		"update-registry": "node scripts/update-registry.js",
		"clean-icons": "node scripts/clean-icons.js",
		"build-icons": "npm run convert-icons && npm run clean-icons && npm run update-registry"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"publint": "^0.3.12",
		"svelte": "^5.0.0",
		"svelte-check": "^3.8.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vite-plugin-dts": "^3.9.0"
	},
	"dependencies": {
		"@fontsource/inter": "^5.2.6",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.36.2",
		"clsx": "^2.1.1",
		"tailwind-merge": "^3.3.1"
	}
}
